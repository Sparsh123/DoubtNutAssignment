<?xml version="1.0" encoding="utf-8"?>
<resources>
    <!-- RoundImageView Custom Styling -->
    <declare-styleable name="RoundImageView">
        <attr name="tt_border_width" format="dimension" />
        <attr name="tt_border_color" format="color" />
        <attr name="tt_border_overlay" format="boolean" />
    </declare-styleable>
    <declare-styleable name="DividerView">
        <attr name="color" format="color" />
        <attr name="dashLength" format="dimension" />
        <attr name="dashGap" format="dimension" />
        <attr name="dashThickness" format="dimension" />
        <attr name="orientation" format="enum">
            <enum name="horizontal" value="0" />
            <enum name="vertical" value="1" />
        </attr>
    </declare-styleable>
</resources>